{
  "folders": {
    "exclude": ["common", "brands", "brands-icons", "companies", "tools", "review-logo", "apps"],
    "include": []
  },
  "files": {
    "exclude": [],
    "include": []
  },
  "extensions": {
    "processable": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
    "videoProcessable": [".mp4", ".webm", ".mov", ".avi", ".mkv"],
    "copyOnly": [".svg"],
    "exclude": []
  },
  "image": {
    "enableResize": true,
    "maxWidth": 1200,
    "quality": {
      "jpeg": 70,
      "png": 8,
      "webp": 70
    }
  },
  "video": {
    "enableProcessing": true,
    "enableResize": true,
    "preserveFormat": true,
    "maxWidth": 1280,
    "maxHeight": 720,
    "quality": {
      "crf": 28,
      "preset": "medium",
      "bitrate": "1000k"
    },
    "formats": {
      "outputFormat": "mp4",
      "codec": "libx264"
    }
  },
  "blur": {
    "strength": 5,
    "folders": {
      "exclude": ["demos", "landing", "media", "common", "brands", "brands-icons", "companies", "tools", "review-logo"],
      "include": []
    },
    "files": {
      "exclude": [],
      "include": []
    }
  }
} 